{
    "name": "graham-campbell/viewer",
    "description": "Viewer Is A View Workaround For Laravel 4.1",
    "keywords": ["laravel", "framework", "view", "workaround", "viewer", "Viewer", "Laravel Viewer", "Laravel-Viewer", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "symfony/config": "2.4.*",
        "symfony/filesystem": "2.4.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.3.*"
    },
    "autoload": {
        "psr-4": {
